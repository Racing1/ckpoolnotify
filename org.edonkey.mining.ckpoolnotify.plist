<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>org.edonkey.mining.ckpoolnotify</string>
	<key>UserName</key>
	<string>REPLACE_ME</string>
	<key>KeepAlive</key>
	<true/>
	<key>RunAtLoad</key>
	<true/>
	<key>StandardOutPath</key>
	<string>/tmp/ckpoolnotify.stdout</string>
	<key>StandardErrorPath</key>
	<string>/tmp/ckpoolnotify.stderr</string>
	<key>ProgramArguments</key>
	<array>
		<string>/Library/Application Support/ckPoolNotify/ckPoolNotifyMac.sh</string>
	</array>
</dict>
</plist>
